accessing
functions: anObject
	"Functions contained in the script that has coverage data.
	type: array of:  $ref(FunctionCoverage)"
	<autoGeneratedMethod>

	functions := anObject collect: [ :each |
		each isDictionary ifTrue:
			[ CDTPFunctionCoverage fromDictionary: each client: client ]
		ifFalse:
			[ each ] ]
