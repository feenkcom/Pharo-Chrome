accessing
positionTicks: anObject
	"An array of source position ticks.
	type: array of:  $ref(PositionTickInfo)
	Flags: optional"
	<autoGeneratedMethod>

	positionTicks := anObject collect: [ :each |
		each isDictionary ifTrue:
			[ CDTPPositionTickInfo fromDictionary: each client: client ]
		ifFalse:
			[ each ] ]
