private - model creation
getMethodSource

	^ String streamContents: [ :stream |
		stream
			<< name; cr;
			<< self methodComment; cr;
			tab; << '<autoGeneratedMethod>'; cr;
			cr; 
			tab; << '^ ';
				<< name.
		self enum ifNotNil: [ :anArray |
			stream 
				<< ' ifNil: [ ''';
				<< anArray first;
				<< ''' ]' ] ].
