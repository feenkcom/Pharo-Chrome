accessing
entries: anObject
	"List of the entries. Specified for `map` and `set` subtype values only.
	type: array of:  $ref(EntryPreview)
	Flags: optional"
	<autoGeneratedMethod>

	entries := anObject collect: [ :each |
		each isDictionary ifTrue:
			[ CDTPEntryPreview fromDictionary: each client: client ]
		ifFalse:
			[ each ] ]
