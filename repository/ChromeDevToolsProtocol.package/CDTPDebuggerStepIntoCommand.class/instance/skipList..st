accessing
skipList: anObject
	"The skipList specifies location ranges that should be skipped on step into.
	type: array of:  $ref(LocationRange)
	Flags: experimental, optional"
	<autoGeneratedMethod>

	skipList := anObject collect: [ :each |
		each isDictionary ifTrue:
			[ CDTPLocationRange fromDictionary: each client: client ]
		ifFalse:
			[ each ] ]
